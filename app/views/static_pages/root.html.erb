<% if logged_in? %>
  <script type="text/javascript">
    window.currentUser = {
      "id": <%= current_user.id %>,
      "fname": "<%= current_user.fname %>",
      "lname": "<%= current_user.lname %>",
      "email" : "<%= current_user.email %>"
    }
  </script>
<% end %>
<main id="root"></main>